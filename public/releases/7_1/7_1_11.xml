  <announcements>
    <announcement lang="en"/>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(Null pointer dereference in zend_mm_alloc_small()).</description>
          <references>
            <reference type="bugfix">75241</reference>
          </references>
        </change>
        <change>
          <description>(infinite loop when printing an error-message).</description>
          <references>
            <reference type="bugfix">75236</reference>
          </references>
        </change>
        <change>
          <description>(Incorrect token formatting on two parse errors in one request).</description>
          <references>
            <reference type="bugfix">75252</reference>
          </references>
        </change>
        <change>
          <description>(Segfault when calling is_callable on parent).</description>
          <references>
            <reference type="bugfix">75220</reference>
          </references>
        </change>
        <change>
          <description>(debug info of Closures of internal functions contain garbage argument names).</description>
          <references>
            <reference type="bugfix">75290</reference>
          </references>
        </change>
      </module>
      <module id="date">
        <change>
          <description>(Out-Of-Bounds Read in timelib_meridian()). (CVE-2017-16642)</description>
          <references>
            <reference type="bugfix">75055</reference>
            <reference type="cve">CVE-2017-16642</reference>
          </references>
        </change>
      </module>
      <module id="apache2handler">
        <change>
          <description>(error: 'zend_hash_key' has no member named 'arKey' in apache2handler).</description>
          <references>
            <reference type="bugfix">75311</reference>
          </references>
        </change>
      </module>
      <module id="hash">
        <change>
          <description>(sha3 hangs on bigendian).</description>
          <references>
            <reference type="bugfix">75303</reference>
          </references>
        </change>
      </module>
      <module id="intl">
        <change>
          <description>(The parameter of UConverter::getAliases() is not optional).</description>
          <references>
            <reference type="bugfix">75318</reference>
          </references>
        </change>
      </module>
      <module id="litespeed">
        <change>
          <description>(Binary directory doesn't get created when building only litespeed SAPI).</description>
          <references>
            <reference type="bugfix">75248</reference>
          </references>
        </change>
        <change>
          <description>(Missing program prefix and suffix).</description>
          <references>
            <reference type="bugfix">75251</reference>
          </references>
        </change>
      </module>
      <module id="mcrypt">
        <change>
          <description>(arcfour encryption stream filter crashes php).</description>
          <references>
            <reference type="bugfix">72535</reference>
          </references>
        </change>
      </module>
      <module id="mysqli">
        <change>
          <description>(Data corruption when reading fields of bit type).</description>
          <references>
            <reference type="bugfix">75018</reference>
          </references>
        </change>
      </module>
      <module id="oci8">
        <change>
          <description>Fixed incorrect reference counting.</description>
          <references/>
        </change>
      </module>
      <module id="opcache">
        <change>
          <description>(Request hangs and not finish).</description>
          <references>
            <reference type="bugfix">75255</reference>
          </references>
        </change>
      </module>
      <module id="pcre">
        <change>
          <description>(applied upstream patch for CVE-2016-1283).</description>
          <references>
            <reference type="bugfix">75207</reference>
            <reference type="cve">CVE-2016-1283</reference>
          </references>
        </change>
      </module>
      <module id="pdo_mysql">
        <change>
          <description>(Type 'bit' is fetched as unexpected string).</description>
          <references>
            <reference type="bugfix">75177</reference>
          </references>
        </change>
      </module>
      <module id="spl">
        <change>
          <description>(SplDoublyLinkedList::setIteratorMode masks intern flags).</description>
          <references>
            <reference type="bugfix">73629</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <tags/>
</release>
