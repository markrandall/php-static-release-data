  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(static embed SAPI linkage error).</description>
          <references>
            <reference type="bugfix">74149</reference>
          </references>
        </change>
        <change>
          <description>(falsely exits with "Out of Memory" when using USE_ZEND_ALLOC=0).</description>
          <references>
            <reference type="bugfix">73370</reference>
          </references>
        </change>
        <change>
          <description>(Leak with instance method calling static method with referenced return).</description>
          <references>
            <reference type="bugfix">73960</reference>
          </references>
        </change>
        <change>
          <description>(Resolution of self::FOO in class constants not correct).</description>
          <references>
            <reference type="bugfix">69676</reference>
          </references>
        </change>
        <change>
          <description>(Build problems after 7.0.17 release: undefined reference to `isfinite').</description>
          <references>
            <reference type="bugfix">74265</reference>
          </references>
        </change>
        <change>
          <description>(yield fromLABEL is over-greedy).</description>
          <references>
            <reference type="bugfix">74302</reference>
          </references>
        </change>
      </module>
      <module id="apache">
        <change>
          <description>Reverted patch for bug #61471, fixes bug #74318.</description>
          <references/>
        </change>
      </module>
      <module id="date">
        <change>
          <description>(Swatch time value incorrect for dates before 1970).</description>
          <references>
            <reference type="bugfix">72096</reference>
          </references>
        </change>
      </module>
      <module id="dom">
        <change>
          <description>(LIBXML_NOWARNING flag ingnored on loadHTML*).</description>
          <references>
            <reference type="bugfix">74004</reference>
          </references>
        </change>
      </module>
      <module id="iconv">
        <change>
          <description>(iconv fails to fail on surrogates).</description>
          <references>
            <reference type="bugfix">74230</reference>
          </references>
        </change>
      </module>
      <module id="opcache">
        <change>
          <description>(OPcache compilation performance regression in PHP 5.6/7 with huge classes).</description>
          <references>
            <reference type="bugfix">74250</reference>
          </references>
        </change>
      </module>
      <module id="openssl">
        <change>
          <description>(fwrite() on non-blocking SSL sockets doesn't work).</description>
          <references>
            <reference type="bugfix">72333</reference>
          </references>
        </change>
      </module>
      <module id="pdo mysql">
        <change>
          <description>(Expose MYSQLI_CLIENT_SSL_DONT_VERIFY_SERVER_CERT to PDO interface).</description>
          <references>
            <reference type="bugfix">71003</reference>
          </references>
        </change>
      </module>
      <module id="spl">
        <change>
          <description>(ArrayObject can not notice changes).</description>
          <references>
            <reference type="bugfix">74058</reference>
          </references>
        </change>
      </module>
      <module id="sqlite">
        <change>
          <description>(Allow creation of deterministic sqlite functions).</description>
          <references>
            <reference type="bugfix">74217</reference>
          </references>
        </change>
      </module>
      <module id="streams">
        <change>
          <description>(Correctly fail on invalid IP address ports).</description>
          <references>
            <reference type="bugfix">74216</reference>
          </references>
        </change>
      </module>
      <module id="zlib">
        <change>
          <description>(deflate_add can allocate too much memory).</description>
          <references>
            <reference type="bugfix">74240</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
