  <announcements>
    <announcement lang="en"/>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(Random "Invalid request (unexpected EOF)" using a router script).</description>
          <references>
            <reference type="bugfix">60471</reference>
          </references>
        </change>
        <change>
          <description>(PHP seems incompatible with OneDrive files on demand).</description>
          <references>
            <reference type="bugfix">75384</reference>
          </references>
        </change>
        <change>
          <description>(Segmentation fault in 7.1.12 and 7.0.26).</description>
          <references>
            <reference type="bugfix">75573</reference>
          </references>
        </change>
      </module>
      <module id="fpm">
        <change>
          <description>(libxml_disable_entity_loader setting is shared between requests).</description>
          <references>
            <reference type="bugfix">64938</reference>
          </references>
        </change>
      </module>
      <module id="gd">
        <change>
          <description>(Potential infinite loop in gdImageCreateFromGifCtx). (CVE-2018-5711)</description>
          <references>
            <reference type="bugfix">75571</reference>
            <reference type="cve">CVE-2018-5711</reference>
          </references>
        </change>
      </module>
      <module id="opcache">
        <change>
          <description>(Interned strings buffer overflow may cause crash).</description>
          <references>
            <reference type="bugfix">75579</reference>
          </references>
        </change>
      </module>
      <module id="pcre">
        <change>
          <description>(preg_last_error not returning error code after error).</description>
          <references>
            <reference type="bugfix">74183</reference>
          </references>
        </change>
      </module>
      <module id="phar">
        <change>
          <description>(Reflected XSS in .phar 404 page). (CVE-2018-5712)</description>
          <references>
            <reference type="bugfix">74782</reference>
            <reference type="cve">CVE-2018-5712</reference>
          </references>
        </change>
      </module>
      <module id="standard">
        <change>
          <description>(Inappropriately parsing HTTP response leads to PHP segment fault). (CVE-2018-14884)</description>
          <references>
            <reference type="bugfix">75535</reference>
            <reference type="cve">CVE-2018-14884</reference>
          </references>
        </change>
        <change>
          <description>(accept EFAULT in addition to ENOSYS as indicator that getrandom() is missing).</description>
          <references>
            <reference type="bugfix">75409</reference>
          </references>
        </change>
      </module>
      <module id="zip">
        <change>
          <description>(Segfault with libzip 1.3.1).</description>
          <references>
            <reference type="bugfix">75540</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <tags>
    <tag>security</tag>
  </tags>
</release>
