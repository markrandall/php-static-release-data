<?xml version="1.0"?>
<release version="5.3.14">
  <version>5.3.14</version>
  <date>2012-06-14</date>
  <sources>
    <source>
      <name>PHP 5.3.14 (tar.bz2)</name>
      <filename>php-5.3.14.tar.bz2</filename>
      <date>2012-06-14</date>
      <md5/>
    </source>
    <source>
      <name>PHP 5.3.14 (tar.gz)</name>
      <filename>php-5.3.14.tar.gz</filename>
      <date>2012-06-14</date>
      <md5/>
