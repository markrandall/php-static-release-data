  <announcements>
    <announcement lang="en">The PHP development team announces the immediate availability of PHP 5.3.27. About 10 bugs were fixed, including a security fix in the XML parser (Bug #65236).</announcement>
  </announcements>
  <tags/>
  <changes>
    <module id="Core">
      <change>
        <description>(segfault in zend_do_fcall_common_helper_SPEC).</description>
        <references>
          <reference type="bugfix">64966</reference>
        </references>
      </change>
      <change>
        <description>(Segfault in gc_zval_possible_root).</description>
        <references>
          <reference type="bugfix">64960</reference>
        </references>
      </change>
      <change>
        <description>(Apache2 TS crash with get_browser()).</description>
        <references>
          <reference type="bugfix">64934</reference>
        </references>
      </change>
      <change>
        <description>(compile failure on netbsd).</description>
        <references>
          <reference type="bugfix">63186</reference>
        </references>
      </change>
    </module>
    <module id="DateTime">
      <change>
        <description>(Crash when using unserialized DatePeriod instance).</description>
        <references>
          <reference type="bugfix">53437</reference>
        </references>
      </change>
    </module>
    <module id="PDO_firebird">
      <change>
        <description>(Firebird return wrong value for numeric field).</description>
        <references>
          <reference type="bugfix">64037</reference>
        </references>
      </change>
      <change>
        <description>(Cannot insert second row with null using parametrized query).</description>
        <references>
          <reference type="bugfix">62024</reference>
        </references>
      </change>
    </module>
    <module id="PDO_pgsql">
      <change>
        <description>(Buffer overflow in _pdo_pgsql_error).</description>
        <references>
          <reference type="bugfix">64949</reference>
        </references>
      </change>
    </module>
    <module id="pgsql">
      <change>
        <description>(pg_convert enum type support).</description>
        <references>
          <reference type="bugfix">64609</reference>
        </references>
      </change>
    </module>
    <module id="SPL">
      <change>
        <description>(Segfault while using RecursiveIteratorIterator on 64-bits systems).</description>
        <references>
          <reference type="bugfix">64997</reference>
        </references>
      </change>
    </module>
    <module id="XML">
      <change>
        <description>(heap corruption in xml parser). (CVE-2013-4113)</description>
        <references>
          <reference type="bugfix">65236</reference>
          <reference type="cve">CVE-2013-4113</reference>
        </references>
      </change>
    </module>
  </changes>
</release>
