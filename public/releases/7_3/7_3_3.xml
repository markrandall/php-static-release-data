<?xml version="1.0"?>
<release version="1" id="7.3.3">
  <release>7.3.3</release>
  <date>07 Mar 2019</date>
  <sources>
    <source>
      <name>PHP 7.3.3 (tar.bz2)</name>
      <filename>php-7.3.3.tar.bz2</filename>
