  <announcements>
    <announcement lang="en">The PHP development team announces the immediate availability of PHP 7.3.12. This is a bug fix release.</announcement>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(Memory corruption using Closure::bindTo).</description>
          <references>
            <reference type="bugfix">78658</reference>
          </references>
        </change>
        <change>
          <description>(Parse errors classified as highest log-level).</description>
          <references>
            <reference type="bugfix">78656</reference>
          </references>
        </change>
        <change>
          <description>(Segfault if GC triggered while generator stack frame is being destroyed).</description>
          <references>
            <reference type="bugfix">78752</reference>
          </references>
        </change>
        <change>
          <description>(Closure::fromCallable() doesn't handle [Closure, '__invoke']).</description>
          <references>
            <reference type="bugfix">78689</reference>
          </references>
        </change>
      </module>
      <module id="com">
        <change>
          <description>(Appending to a variant array causes segfault).</description>
          <references>
            <reference type="bugfix">78694</reference>
          </references>
        </change>
      </module>
      <module id="date">
        <change>
          <description>(\DateInterval incorrectly unserialized).</description>
          <references>
            <reference type="bugfix">70153</reference>
          </references>
        </change>
        <change>
          <description>(Serialising DatePeriod converts DateTimeImmutable).</description>
          <references>
            <reference type="bugfix">78751</reference>
          </references>
        </change>
      </module>
      <module id="iconv">
        <change>
          <description>(Wrong libiconv version displayed).</description>
          <references>
            <reference type="bugfix">78642</reference>
          </references>
        </change>
      </module>
      <module id="opcache">
        <change>
          <description>(Incorrectly computed opcache checksum on files with non-ascii characters).</description>
          <references>
            <reference type="bugfix">78654</reference>
          </references>
        </change>
        <change>
          <description>(OpCache corrupts custom extension result).</description>
          <references>
            <reference type="bugfix">78747</reference>
          </references>
        </change>
      </module>
      <module id="openssl">
        <change>
          <description>(TLS issues from HTTP request affecting other encrypted connections).</description>
          <references>
            <reference type="bugfix">78775</reference>
          </references>
        </change>
      </module>
      <module id="reflection">
        <change>
          <description>(ReflectionClass::ImplementsInterface - inaccurate error message with traits).</description>
          <references>
            <reference type="bugfix">78697</reference>
          </references>
        </change>
      </module>
      <module id="sockets">
        <change>
          <description>(Multicasting may leak memory).</description>
          <references>
            <reference type="bugfix">78665</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <tags/>
</release>
