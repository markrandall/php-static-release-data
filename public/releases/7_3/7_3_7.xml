  <announcements>
    <announcement lang="en">The PHP development team announces the immediate availability of PHP 7.3.7. This is a bug fix release.</announcement>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(Interface gets skipped if autoloader throws an exception).</description>
          <references>
            <reference type="bugfix">76980</reference>
          </references>
        </change>
      </module>
      <module id="dom">
        <change>
          <description>(segfault when accessing properties of DOMDocumentType).</description>
          <references>
            <reference type="bugfix">78025</reference>
          </references>
        </change>
      </module>
      <module id="mysqli">
        <change>
          <description>(When mysqli.allow_local_infile = Off, use a meaningful error message).</description>
          <references>
            <reference type="bugfix">77956</reference>
          </references>
        </change>
        <change>
          <description>(bindParam incorrect processing of bool types).</description>
          <references>
            <reference type="bugfix">38546</reference>
          </references>
        </change>
      </module>
      <module id="mysqlnd">
        <change>
          <description>(Random segmentation fault in mysqlnd from php-fpm).</description>
          <references>
            <reference type="bugfix">77955</reference>
          </references>
        </change>
      </module>
      <module id="opcache">
        <change>
          <description>(Incorrect evaluation of expressions involving partials arrays in SCCP).</description>
          <references>
            <reference type="bugfix">78015</reference>
          </references>
        </change>
        <change>
          <description>(Path resolution fails if opcache disabled during request).</description>
          <references>
            <reference type="bugfix">78106</reference>
          </references>
        </change>
      </module>
      <module id="openssl">
        <change>
          <description>(openssl_encrypt_ccm.phpt fails with OpenSSL 1.1.1c).</description>
          <references>
            <reference type="bugfix">78079</reference>
          </references>
        </change>
      </module>
      <module id="phpdbg">
        <change>
          <description>(SegFault phpdbg + opcache on include file twice).</description>
          <references>
            <reference type="bugfix">78050</reference>
          </references>
        </change>
      </module>
      <module id="sockets">
        <change>
          <description>(Socket_select fails when resource array contains references).</description>
          <references>
            <reference type="bugfix">78038</reference>
          </references>
        </change>
      </module>
      <module id="sodium">
        <change>
          <description>(segfault when calling sodium_* functions from eval).</description>
          <references>
            <reference type="bugfix">78114</reference>
          </references>
        </change>
      </module>
      <module id="standard">
        <change>
          <description>(Extract with EXTR_SKIP should skip $this).</description>
          <references>
            <reference type="bugfix">77135</reference>
          </references>
        </change>
        <change>
          <description>(preg_match failed).</description>
          <references>
            <reference type="bugfix">77937</reference>
          </references>
        </change>
      </module>
      <module id="zip">
        <change>
          <description>(zip.h not found).</description>
          <references>
            <reference type="bugfix">76345</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
  <tags/>
</release>
