    <source>
      <name>PHP 5.5.26 (tar.xz)</name>
      <filename>php-5.5.26.tar.xz</filename>
      <date>11 Jun 2015</date>
      <sha256>97672c41cf2f95628dbffb63648147b43b23ea41b99ad22ccf5f4fe9b6e91b51</sha256>
    </source>
  </sources>
  <announcements>
    <announcement lang="en"/>
  </announcements>
  <tags/>
  <changes>
    <module id="Core">
      <change>
        <description>(Conditional jump or move depends on uninitialised value in extension trait).</description>
        <references>
          <reference type="bugfix">69566</reference>
        </references>
      </change>
      <change>
        <description>(temp. directory is cached during multiple requests).</description>
        <references>
          <reference type="bugfix">66048</reference>
        </references>
      </change>
      <change>
        <description>(complex GLOB_BRACE fails on Windows).</description>
        <references>
          <reference type="bugfix">69628</reference>
        </references>
      </change>
      <change>
        <description>(OS command injection vulnerability in escapeshellarg). (CVE-2015-4642)</description>
        <references>
          <reference type="bugfix">69646</reference>
          <reference type="cve">CVE-2015-4642</reference>
        </references>
      </change>
      <change>
        <description>(Incorrect handling of paths with NULs). (CVE-2015-4598)</description>
        <references>
          <reference type="bugfix">69719</reference>
          <reference type="cve">CVE-2015-4598</reference>
        </references>
      </change>
    </module>
    <module id="FTP">
      <change>
        <description>Improved fix for bug [#69545](http://bugs.php.net/69545) (Integer overflow in ftp_genlist() resulting in heap overflow). (CVE-2015-4643)</description>
        <references>
          <reference type="cve">CVE-2015-4643</reference>
        </references>
      </change>
    </module>
    <module id="GD">
      <change>
        <description>(GD fails to build with newer libvpx).</description>
        <references>
          <reference type="bugfix">69479</reference>
        </references>
      </change>
    </module>
    <module id="Iconv">
      <change>
        <description>(iconv with //IGNORE cuts the string).</description>
        <references>
          <reference type="bugfix">48147</reference>
        </references>
      </change>
    </module>
    <module id="Litespeed SAPI">
      <change>
        <description>(Unchecked return value).</description>
        <references>
          <reference type="bugfix">68812</reference>
        </references>
      </change>
    </module>
    <module id="Mail">
      <change>
        <description>(mail() does not have mail header injection prevention for additional headers).</description>
        <references>
          <reference type="bugfix">68776</reference>
        </references>
      </change>
    </module>
    <module id="MCrypt">
      <change>
        <description>Added file descriptor caching to mcrypt_create_iv().</description>
        <references/>
      </change>
    </module>
    <module id="Opcache">
      <change>
        <description>(Memory leak with opcache.optimization_level=0xFFFFFFFF).</description>
        <references>
          <reference type="bugfix">69549</reference>
        </references>
      </change>
    </module>
    <module id="PCRE">
      <change>
        <description>Upgraded pcrelib to 8.37. (CVE-2015-2325, CVE-2015-2326)</description>
        <references>
          <reference type="cve">CVE-2015-2325</reference>
        </references>
      </change>
    </module>
    <module id="Phar">
      <change>
        <description>(phar symlink in binary directory broken).</description>
        <references>
          <reference type="bugfix">69680</reference>
        </references>
      </change>
    </module>
    <module id="Postgres">
      <change>
        <description>(segfault in php_pgsql_meta_data). (CVE-2015-4644)</description>
        <references>
          <reference type="bugfix">69667</reference>
          <reference type="cve">CVE-2015-4644</reference>
        </references>
      </change>
    </module>
    <module id="Sqlite3">
      <change>
        <description>Upgrade bundled sqlite to 3.8.10.2. (CVE-2015-3414, CVE-2015-3415, CVE-2015-3416)</description>
        <references>
          <reference type="cve">CVE-2015-3414</reference>
        </references>
      </change>
    </module>
  </changes>
</release>
