  <changes>
    <module id="Core">
      <change>
        <description>(glob returns error, should be empty array()).</description>
        <references>
          <reference type="bugfix">47358</reference>
        </references>
      </change>
      <change>
        <description>(SIGSEGV during zend_shutdown()).</description>
        <references>
          <reference type="bugfix">65463</reference>
        </references>
      </change>
      <change>
        <description>(Crash on SIGTERM in apache process).</description>
        <references>
          <reference type="bugfix">66036</reference>
        </references>
      </change>
      <change>
        <description>(program_prefix not honoured in man pages).</description>
        <references>
          <reference type="bugfix">67878</reference>
        </references>
      </change>
    </module>
    <module id="COM">
      <change>
        <description>(DOTNET is successful once per server run).</description>
        <references>
          <reference type="bugfix">41577</reference>
        </references>
      </change>
    </module>
    <module id="Date">
      <change>
        <description>(memory leaks in DateTime constructor).</description>
        <references>
          <reference type="bugfix">66091</reference>
        </references>
      </change>
      <change>
        <description>(Some timezones are no longer valid in PHP 5.5.10).</description>
        <references>
          <reference type="bugfix">66985</reference>
        </references>
      </change>
      <change>
        <description>(First uppercase letter breaks date string parsing).</description>
        <references>
          <reference type="bugfix">67109</reference>
        </references>
      </change>
    </module>
    <module id="FPM">
      <change>
        <description>(FPM with mod_fastcgi/apache2.4 is broken).</description>
        <references>
          <reference type="bugfix">67606</reference>
        </references>
      </change>
    </module>
    <module id="GD">
      <change>
        <description>Made fontFetch's path parser thread-safe.</description>
        <references/>
      </change>
    </module>
    <module id="MySQLi">
      <change>
        <description>(mysqli does not handle 4-byte floats correctly).</description>
        <references>
          <reference type="bugfix">67839</reference>
        </references>
      </change>
    </module>
    <module id="OpenSSL">
      <change>
        <description>(socket timeouts not honored in blocking SSL reads).</description>
        <references>
          <reference type="bugfix">41631</reference>
        </references>
      </change>
      <change>
        <description>(extension won't build if openssl compiled without SSLv3).</description>
        <references>
          <reference type="bugfix">67850</reference>
        </references>
      </change>
    </module>
    <module id="SPL">
      <change>
        <description>(CachingIterator::__construct InvalidArgumentException wrong message).</description>
        <references>
          <reference type="bugfix">67813</reference>
        </references>
      </change>
    </module>
    <module id="Zlib">
      <change>
        <description>(chained zlib filters silently fail with large amounts of data).</description>
        <references>
          <reference type="bugfix">67724</reference>
        </references>
      </change>
      <change>
        <description>(internal corruption phar error).</description>
        <references>
          <reference type="bugfix">67865</reference>
        </references>
      </change>
    </module>
