  <announcements>
    <announcement lang="en"/>
  </announcements>
  <changes>
    <modules>
      <module id="core">
        <change>
          <description>(configure fails utf8_to_mutf7 test).</description>
          <references>
            <reference type="bugfix">66909</reference>
          </references>
        </change>
        <change>
          <description>(Invalid opcode while using ::class as trait method paramater default value).</description>
          <references>
            <reference type="bugfix">70958</reference>
          </references>
        </change>
        <change>
          <description>(self::class can not be resolved with reflection for abstract class).</description>
          <references>
            <reference type="bugfix">70957</reference>
          </references>
        </change>
        <change>
          <description>(try{ } finally{} can create infinite chains of exceptions).</description>
          <references>
            <reference type="bugfix">70944</reference>
          </references>
        </change>
        <change>
          <description>(SAPI build problem on AIX: Undefined symbol: php_register_internal_extensions).</description>
          <references>
            <reference type="bugfix">61751</reference>
          </references>
        </change>
      </module>
      <module id="fpm">
        <change>
          <description>(fpm_log.c memory leak and buffer overflow). (CVE-2016-5114)</description>
          <references>
            <reference type="bugfix">70755</reference>
            <reference type="cve">CVE-2016-5114</reference>
          </references>
        </change>
      </module>
      <module id="gd">
        <change>
          <description>(Memory Read via gdImageRotateInterpolated Array Index Out of Bounds). (CVE-2016-1903)</description>
          <references>
            <reference type="bugfix">70976</reference>
            <reference type="cve">CVE-2016-1903</reference>
          </references>
        </change>
      </module>
      <module id="mysqlnd">
        <change>
          <description>(LOAD DATA LOCAL INFILE / open_basedir restriction).</description>
          <references>
            <reference type="bugfix">68077</reference>
          </references>
        </change>
      </module>
      <module id="soap">
        <change>
          <description>(SoapClient systematic out of memory error).</description>
          <references>
            <reference type="bugfix">70900</reference>
          </references>
        </change>
      </module>
      <module id="standard">
        <change>
          <description>(ReflectionFunction for array_unique returns wrong number of parameters).</description>
          <references>
            <reference type="bugfix">70960</reference>
          </references>
        </change>
      </module>
      <module id="pdo_firebird">
        <change>
          <description>(Integer returned as a 64bit integer on X64_86).</description>
          <references>
            <reference type="bugfix">60052</reference>
          </references>
        </change>
      </module>
      <module id="wddx">
        <change>
          <description>(Use After Free Vulnerability in WDDX Packet Deserialization).</description>
          <references>
            <reference type="bugfix">70661</reference>
          </references>
        </change>
        <change>
          <description>(Session WDDX Packet Deserialization Type Confusion Vulnerability).</description>
          <references>
            <reference type="bugfix">70741</reference>
          </references>
        </change>
      </module>
      <module id="xmlrpc">
        <change>
          <description>(Type Confusion Vulnerability in PHP_to_XMLRPC_worker()).</description>
          <references>
            <reference type="bugfix">70728</reference>
          </references>
        </change>
      </module>
    </modules>
  </changes>
