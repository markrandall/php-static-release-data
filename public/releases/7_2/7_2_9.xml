  <announcements>
    <announcement lang="en">The PHP development team announces the immediate availability of PHP 7.2.9. This is a bugfix release.</announcement>
  </announcements>
  <tags/>
  <changes>
    <module id="Core">
      <change>
        <description>(jewish.c: compile error under Windows with GBK charset).</description>
        <references>
          <reference type="bugfix">52974</reference>
        </references>
      </change>
    </module>
    <module id="Filter">
      <change>
        <description>(References in sub-array for filtering breaks the filter).</description>
        <references>
          <reference type="bugfix">76366</reference>
        </references>
      </change>
    </module>
    <module id="PDO_Firebird">
      <change>
        <description>(Memory leak when fetching a BLOB field).</description>
        <references>
          <reference type="bugfix">76488</reference>
        </references>
      </change>
    </module>
    <module id="PDO_PgSQL">
      <change>
        <description>(Possible Memory Leak using PDO::CURSOR_SCROLL option).</description>
        <references>
          <reference type="bugfix">75402</reference>
        </references>
      </change>
    </module>
    <module id="SQLite3">
      <change>
        <description>(SQLite3Stmt::bindValue() with SQLITE3_FLOAT doesn't juggle).</description>
        <references>
          <reference type="bugfix">76665</reference>
        </references>
      </change>
    </module>
    <module id="Standard">
      <change>
        <description>(Incorrect entries in get_html_translation_table).</description>
        <references>
          <reference type="bugfix">73817</reference>
        </references>
      </change>
      <change>
        <description>(array_column: null values in $index_key become incrementing keys in result).</description>
        <references>
          <reference type="bugfix">68553</reference>
        </references>
      </change>
      <change>
        <description>(Segmentation fault when using `output_add_rewrite_var`).</description>
        <references>
          <reference type="bugfix">76643</reference>
        </references>
      </change>
    </module>
    <module id="Zip">
      <change>
        <description>(ZipArchive memory leak (OVERWRITE flag and empty archive)).</description>
        <references>
          <reference type="bugfix">76524</reference>
        </references>
      </change>
    </module>
  </changes>
</release>
