    <module id="MySQLi">
      <change>
        <description>(Segfault calling bind_param() on mysqli).</description>
        <references>
          <reference type="bugfix">66043</reference>
        </references>
      </change>
    </module>
    <module id="mysqlnd">
      <change>
        <description>(mysqli under mysqlnd loses precision when bind_param with 'i').</description>
        <references>
          <reference type="bugfix">66124</reference>
        </references>
      </change>
      <change>
        <description>(mysqlnd quote function is wrong with NO_BACKSLASH_ESCAPES after failed query).</description>
        <references>
          <reference type="bugfix">66141</reference>
